{
    "name": "tobb10001/h4a-wordpress",
    "description": "Wordpress Plugin to view data from Handball4All.",
    "license": "LGPL-3.0-only",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Tobb10001\\H4aWordpress\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tobias Fischer",
            "email": "tobb10001@gmail.com"
        }
